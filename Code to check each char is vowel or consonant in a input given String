void main() {
  String input = "Madhurima1";
  String vowels = "aeiouAEIOU";

  for (int i = 0; i < input.length; i++) {
    String ch = input[i];

    if (RegExp(r'[a-zA-Z]').hasMatch(ch)) {
      if (vowels.contains(ch)) {
        print("Vowel: $ch");
      } else {
        print("Consonant: $ch");
      }
    } else {
      print("Not a letter: $ch");
    }
  }
}
